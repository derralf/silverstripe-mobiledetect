{
  "name": "derralf/silverstripe-mobiledetect",
  "description": "Simple extension to detect mobile devices",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "mobile detect"
  ],
  "license": "BSD-3-Clause",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "silverstripe/framework": "^5",
    "mobiledetect/mobiledetectlib": "^4.8"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Derralf\\MobileDetect\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
